{
  "name": "crossfeed-docs",
  "description": "Crossfeed Documentation",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "clean": "rimraf .cache public _site",
    "build": "gatsby build",
    "reset": "npx rimraf .git",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "prettier --check \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "codegen": "swagger-jsdoc -d ./src/swaggerDef.js -o ./src/generated/swagger.json",
    "postinstall": "npx npm-force-resolutions && npm run codegen"
  },
  "dependencies": {
    "btoa": "^1.2.1",
    "react-uswds": "^0.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "classnames": "^2.2.6",
    "clipboardy": "^3.0.0",
    "cross-spawn": "^7.0.3",
    "eslint-config-react-app": "^5.2.1",
    "gatsby": "^5.8.1",
    "gatsby-awesome-pagination": "^0.3.8",
    "gatsby-cli": "^5.8.0",
    "gatsby-core-utils": "^4.8.0",
    "gatsby-plugin-manifest": "^5.8.0",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-react-helmet": "^6.8.0",
    "gatsby-plugin-sass": "^6.8.0",
    "gatsby-plugin-sharp": "^5.8.1",
    "gatsby-remark-autolink-headers": "^6.8.0",
    "gatsby-remark-images": "^7.8.0",
    "gatsby-remark-prismjs": "^7.8.0",
    "gatsby-source-filesystem": "^5.8.0",
    "gatsby-telemetry": "^4.8.0",
    "gatsby-transformer-remark": "^6.8.0",
    "get-stream": "^4.1.0",
    "html-loader": "^1.3.2",
    "klona": "^2.0.6",
    "nanoid": "^4.0.2",
    "password-prompt": "^1.1.2",
    "prettier": "^2.1.2",
    "prismjs": "^1.27.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.61.0",
    "sass-loader": "^13.2.2",
    "swagger-jsdoc": "^5.0.1",
    "swagger-ui-react": "^4.18.2",
    "uswds": "^2.13.3",
    "uswds-gulp": "^0.0.1-security",
    "webpack": "^5.78.0",
    "xss": "^1.0.10"
  },
  "resolutions": {
    "trim": "0.0.3",
    "sanitize-html": "2.10.0",
    "ssri": "8.0.1",
    "parse-url": "8.1.0"
  }
}
